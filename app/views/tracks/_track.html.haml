- cache(track) do
  %tr
    %td= track.artist_name
    %td= track.name
    %td= track.average_rating
    %td= track.releases
    %td= link_to("iTunes", itunes_track_path(track))
    %td
      = form_for track do |f|
        = f.fields_for :ratings do |builder|
          = render "ratings/nestedform", f: builder
        = f.submit
